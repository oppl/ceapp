<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="current_timestamp" dbms="postgresql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity CollisionLevelThree.
    -->
    <changeSet id="20170716102450-1" author="jhipster">
        <createTable tableName="collision_level_three">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="exam_collision" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="institute_collision" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="curriculum_collision" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="collision_value_avg" type="double">
                <constraints nullable="true" />
            </column>

            <column name="collision_value_max" type="double">
                <constraints nullable="true" />
            </column>

            <column name="col_ws" type="boolean">
                <constraints nullable="true" />
            </column>

            <column name="col_ss" type="boolean">
                <constraints nullable="true" />
            </column>

            <column name="count_collision_lvas" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="collision_level_two_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="curriculum_subject_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
